<% like = current_user.profile.likes.find_by(likeable: p.id) %>
<%= turbo_stream.replace "likeable_options_#{p.id}" do %>
  <%= turbo_frame_tag "likeable_options_#{p.id}" do %>
    <%= link_to like_path(like, post_id: p), data: { turbo_method: :delete } do %>
      <i class="fa-solid fa-heart fa-lg"></i>
    <% end %>
    <%= p.likes.count %>
  <% end %>
<% end %>