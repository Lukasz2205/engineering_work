<div class="columns is-12 normal-margin-top">
  <div class="column is-12">
    <div class="columns is-12" style="flex-direction: column; height: 100vh;">
      <%= turbo_frame_tag "posts-list" do %>
        <% @posts.each do |p| %>
          <%= render 'shared/profile_post', p: p %>
        <% end %>
      <% end %>
    </div>
  </div>
</div>